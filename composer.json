{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/admin_toolbar": "~2.0",
        "drupal/coffee": "~1.0",
        "drupal/config_split": "~1.0",
        "composer/installers": "^1.0.20",
        "drupal/core-composer-scaffold": "^8.8 || ~9.0",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "^8.8 || ~9.0",
        "drush/drush": "~9.0",
        "drupal/console": "~1.0",
        "drupal/panels": "^4.6",
        "drupal/page_manager": "^4.0@beta",
        "drupal/field_group": "^3.0",
        "drupal/pathauto": "^1.6",
        "drupal/paragraphs": "1.11",
        "drupal/crop": "2.1",
        "drupal/webform": "^5.9",
        "drupal/bootstrap_paragraphs": "^2.0@beta",
        "drupal/image_widget_crop": "^2.2",
        "drupal/bootstrap": "^3.21",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/views_bootstrap": "4.2",
        "drupal/radix_layouts": "^4.1",
        "drupal/components": "^1.1",
        "drupal/radix": "^4.8",
        "drupal/fixed_block_content": "^1.0",
        "drupal/google_analytics": "^3.0",
        "drupal/google_tag": "1.1",
        "drupal/extlink": "^1.3",
        "drupal/menu_trail_by_path": "1.2",
        "drupal/video_embed_field": "2.3",
        "drupal/slick": "^1.1",
        "drupal/slick_views": "^2.0",
        "drupal/slick_extras": "^1.0@RC",
        "drupal/svg_image": "1.12",
        "drupal/bg_image_formatter": "1.13",
        "drupal/deploy": "^1.0@beta",
        "drupal/relaxed": "^1.0@beta",
        "drupal/fontyourface": "^3.2",
        "drupal/health_check": "1.2",
        "drupal/twig_tweak": "2.5",
        "drupal/backup_migrate": "4.1",
        "drupal/social_api": "2.x-dev",
        "drupal/social_auth": "2.x-dev",
        "drupal/social_auth_google": "2.x-dev",
        "drupal/instapage_cms_plugin": "^3.18",
        "drupal/metatag": "1.11",
        "drupal/honeypot": "1.30",
        "drupal/keycdn": "^1.0",
        "drupal/redirect": "1.5",
        "drupal/advagg": "^4.0",
        "drupal/antibot": "1.3",
        "drupal/layout_builder_styles": "^1.0@beta"
    },
    "require-dev": {
        "drupal/default_content_deploy": "1.0.0-alpha5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patches-file": "composer.patches.json",
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    },
    "replace": {
        "drupal/drupal": "*"
    },
    "config": {
        "process-timeout": 2000
    }
}
